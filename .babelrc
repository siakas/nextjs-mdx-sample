{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      }
    ]
  ],
  "plugins": [
    [
      "@emotion",
      {
        // ソースマップはデフォルトで有効化されているが、製品版デプロイでは削除される
        "sourceMap": true,
        // "autoLabel": "always",
        "labelFormat": "[filename]-[local]",
        "cssPropOptimization": true
      }
    ]
  ]
}
